doctype html
html(lang="en")
    head
        block head
            include head.html
    body
        block navigation
            include nav.jade
        block body
            .container
                .row
                    main.col-sm-8
                        for post in posts
                            article
                                = post
                    aside.col-sm-3.col-sm-offset-1
                        include sidebar.jade
        footer
            p A Lynn Cyrin Website
            p: a(href='#') Back to top
        block footer
            include footer.html
